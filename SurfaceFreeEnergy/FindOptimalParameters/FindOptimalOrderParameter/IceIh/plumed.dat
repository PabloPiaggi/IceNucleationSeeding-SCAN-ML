# vim:ft=plumed

ENVIRONMENTSIMILARITY ...
 SPECIES=1-864:3
 SIGMA=0.065
 CRYSTAL_STRUCTURE=CUSTOM
 LABEL=refcv
 REFERENCE_1=env1h.pdb
 REFERENCE_2=env2h.pdb
 REFERENCE_3=env3h.pdb
 REFERENCE_4=env4h.pdb
 MORE_THAN={CUBIC D_0=0.447 D_MAX=0.949}
 MEAN
... ENVIRONMENTSIMILARITY

hh: HISTOGRAM ...
   DATA=refcv
   GRID_MIN=-0.5
   GRID_MAX=2
   GRID_BIN=1000
   BANDWIDTH=0.01
...

DUMPGRID GRID=hh FILE=histo STRIDE=100

PRINT STRIDE=1  ARG=* FILE=COLVAR
